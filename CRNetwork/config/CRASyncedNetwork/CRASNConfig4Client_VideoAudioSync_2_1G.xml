<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="NCS"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/>
    <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/>
    <!-- yes, no -->
	<Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourceVideo" mediatype="RawVideoRGB"/>
      <Connection ASNNode ="ASNNode/Data/SourceVideo" SyncNode="SyncNode/SourceVideo01" CodecNode="CodecNode/SourceVideo01" CommNode="CommNode/SourceVideo"/>
    </ASNLink>    
    <ASNLink>
      <Property name="ASNDestinationVideo" mediatype="RawVideoRGB"/>
      <Connection ASNNode ="ASNNode/Data/DestinationVideo" SyncNode="SyncNode/DestinationVideo01" CodecNode="CodecNode/DestinationVideo01" CommNode="CommNode/DestinationVideo"/>
    </ASNLink>
    <ASNLink>
      <Property name ="ASNSourceAudio" mediatype="RawSound"/>
      <Connection ASNNode ="ASNNode/Data/SourceAudio" SyncNode="SyncNode/SourceAudio01" CodecNode="CodecNode/SourceAudio01" CommNode="CommNode/SourceAudio"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationAudio" mediatype="RawSound"/>
      <Connection ASNNode ="ASNNode/Data/DestinationAudio" SyncNode="SyncNode/DestinationAudio01" CodecNode="CodecNode/DestinationAudio01" CommNode="CommNode/DestinationAudio"/>
    </ASNLink>
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourceVideo"/>
      <Source name="ASNNode/Data/SourceAudio"/>
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationVideo"/>
      <Destination name="ASNNode/Data/DestinationAudio"/>
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceVideo01"/>
      <Source name="SyncNode/SourceAudio01"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationVideo01"/>
      <Destination name="SyncNode/DestinationAudio01"/>
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
    <SourceList>
      <Source name="CodecNode/SourceVideo01"/>
      <Source name="CodecNode/SourceAudio01"/>
    </SourceList>
    <DestinationList>
      <Destination name="CodecNode/DestinationVideo01"/>
      <Destination name="CodecNode/DestinationAudio01"/>
    </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	    <Source name="CommNode/SourceVideo"/>
      <Source name="CommNode/SourceAudio"/>
     </SourceList>
	 <DestinationList>
	   <Destination name="CommNode/DestinationVideo" />
     <Destination name="CommNode/DestinationAudio" />
	 </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig4Client_VideoAudioSync_2_1G.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfigVideoAudio.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
