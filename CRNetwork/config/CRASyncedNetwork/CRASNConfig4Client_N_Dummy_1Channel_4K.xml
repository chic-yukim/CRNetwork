<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="N"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/>    <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/>    <!-- yes, no -->
    <Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourcePhysics" mediatype="PhysicsRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourcePhysics" SyncNode="SyncNode/SourcePhysics" CodecNode="CodecNode/SourcePhysics01" CommNode="CommNode/SourcePhysics"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationPhysics" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationPhysics" SyncNode="SyncNode/DestinationPhysics" CodecNode="CodecNode/DestinationPhysics01" CommNode="CommNode/DestinationPhysics"/>
    </ASNLink>
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourcePhysics"/>
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationPhysics"/>
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourcePhysics"/>      
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationPhysics"/>      
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourcePhysics01"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationPhysics01"/>
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	  <Source name="CommNode/SourcePhysics"/>
    </SourceList>
	  <DestinationList>
		  <Destination name="CommNode/DestinationPhysics" />
	  </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig4Client_N_Dummy_1Channel_4K.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfig.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
