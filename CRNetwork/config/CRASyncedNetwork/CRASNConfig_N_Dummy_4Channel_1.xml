<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="N"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/> <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/> <!-- yes, no -->
	<Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourceVideo" mediatype="VideoRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceVideo" SyncNode="SyncNode/SourceVideo" CodecNode="CodecNode/SourceVideo01" CommNode="CommNode/SourceVideo"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationVideo21" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationVideo" SyncNode="SyncNode/DestinationVideo1" CodecNode="CodecNode/DestinationVideo01" CommNode="CommNode/DestinationVideo21"/>
    </ASNLink>
	<ASNLink>
      <Property name="ASNSourceAudio" mediatype="AudioRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceAudio" SyncNode="SyncNode/SourceAudio" CodecNode="CodecNode/SourceAudio01" CommNode="CommNode/SourceAudio"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationAudio21" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationAudio" SyncNode="SyncNode/DestinationAudio1" CodecNode="CodecNode/DestinationAudio01" CommNode="CommNode/DestinationAudio21"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourcePhysics" mediatype="VideoRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourcePhysics" SyncNode="SyncNode/SourcePhysics" CodecNode="CodecNode/SourcePhysics01" CommNode="CommNode/SourcePhysics"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationPhysics21" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationPhysics" SyncNode="SyncNode/DestinationPhysics1" CodecNode="CodecNode/DestinationPhysics01" CommNode="CommNode/DestinationPhysics21"/>
    </ASNLink>
	<ASNLink>
      <Property name="ASNSourceControl" mediatype="AudioRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceControl" SyncNode="SyncNode/SourceControl" CodecNode="CodecNode/SourceControl01" CommNode="CommNode/SourceControl"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationControl21" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationControl" SyncNode="SyncNode/DestinationControl1" CodecNode="CodecNode/DestinationControl01" CommNode="CommNode/DestinationControl21"/>
    </ASNLink>
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourceVideo"/>
      <Source name="ASNNode/Data/SourceAudio"/>
      <Source name="ASNNode/Data/SourcePhysics"/>
      <Source name="ASNNode/Data/SourceControl"/>
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationVideo"/>
      <Destination name="ASNNode/Data/DestinationAudio"/>
      <Destination name="ASNNode/Data/DestinationPhysics"/>
      <Destination name="ASNNode/Data/DestinationControl"/>
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceVideo"/>        
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationVideo"/>        
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourceVideo01"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationVideo01"/>
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	  <Source name="CommNode/SourceVideo"/>
	  <Source name="CommNode/SourceAudio"/>
	  <Source name="CommNode/SourcePhysics"/>
	  <Source name="CommNode/SourceControl"/>
    </SourceList>
	  <DestinationList>
		  <Destination name="CommNode/DestinationVideo21" />
		  <Destination name="CommNode/DestinationAudio21" />
		  <Destination name="CommNode/DestinationPhysics21" />
		  <Destination name="CommNode/DestinationControl21" />
	  </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig_N_4Channel_1.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfig.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
