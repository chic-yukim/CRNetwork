<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="N"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/>
    <!--Attach, Detach-->
    <Info name ="PrintError" value="no"/>
    <!-- yes, no -->
	<Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourceVideo01" mediatype="VideoRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceVideo01" SyncNode="SyncNode/SourceVideo" CodecNode="CodecNode/SourceVideo" CommNode="CommNode/SourceVideo01"/>
    </ASNLink>
    <ASNLink>
      <Property name ="ASNSourceAudio01" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/SourceAudio01" SyncNode="SyncNode/SourceAudio" CodecNode="CodecNode/SourceAudio" CommNode="CommNode/SourceAudio01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourcePhysics01"  mediatype=""/>
      <Connection ASNNode="ASNNode/Data/SourcePhysics01" SyncNode="SyncNode/SourcePhysics" CodecNode="None" CommNode="CommNode/SourcePhysics01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourceControl01" mediatype=""/>
      <Connection ASNNode="ASNNode/Data/SourceControl01" SyncNode="SyncNode/SourceControl" CodecNode="None" CommNode="CommNode/SourceControl01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationVideo01" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationVideo01" SyncNode="SyncNode/DestinationVideo" CodecNode="CodecNode/DestinationVideo" CommNode="CommNode/DestinationVideo01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationAudio01" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationAudio01" SyncNode="SyncNode/DestinationAudio" CodecNode="CodecNode/DestinationAudio" CommNode="CommNode/DestinationAudio01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationPhysics01" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationPhysics01" SyncNode="SyncNode/DestinationPhysics" CodecNode="None" CommNode="CommNode/DestinationPhysics01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationControl01" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationControl01" SyncNode="SyncNode/DestinationControl" CodecNode="None" CommNode="CommNode/DestinationControl01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourceVideo02" mediatype="VideoRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceVideo02" SyncNode="SyncNode/SourceVideo" CodecNode="CodecNode/SourceVideo" CommNode="CommNode/SourceVideo02"/>
    </ASNLink>
    <ASNLink>
      <Property name ="ASNSourceAudio02" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/SourceAudio02" SyncNode="SyncNode/SourceAudio" CodecNode="CodecNode/SourceAudio" CommNode="CommNode/SourceAudio02"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourcePhysics02"  mediatype=""/>
      <Connection ASNNode="ASNNode/Data/SourcePhysics02" SyncNode="SyncNode/SourcePhysics" CodecNode="None" CommNode="CommNode/SourcePhysics02"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourceControl02" mediatype=""/>
      <Connection ASNNode="ASNNode/Data/SourceControl02" SyncNode="SyncNode/SourceControl" CodecNode="None" CommNode="CommNode/SourceControl02"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationVideo02" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationVideo02" SyncNode="SyncNode/DestinationVideo" CodecNode="CodecNode/DestinationVideo" CommNode="CommNode/DestinationVideo02"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationAudio02" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationAudio02" SyncNode="SyncNode/DestinationAudio" CodecNode="CodecNode/DestinationAudio" CommNode="CommNode/DestinationAudio02"/>
