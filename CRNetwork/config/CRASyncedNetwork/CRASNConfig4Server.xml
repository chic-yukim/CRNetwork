    <ASNLink>
      <Property name="ASNDestinationControl02" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationControl02" SyncNode="SyncNode/DestinationControl" CodecNode="None" CommNode="CommNode/DestinationControl02"/>
    </ASNLink>
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourceVideo01"/>
      <Source name="ASNNode/Data/SourceAudio01"/>
      <Source name="ASNNode/Data/SourcePhysics01"/>
      <Source name="ASNNode/Data/SourceControl01"/>
      <Source name="ASNNode/Data/SourceVideo02"/>
      <Source name="ASNNode/Data/SourceAudio02"/>
      <Source name="ASNNode/Data/SourcePhysics02"/>
      <Source name="ASNNode/Data/SourceControl02"/>
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationVideo01"/>
      <Destination name="ASNNode/Data/DestinationAudio01"/>
      <Destination name="ASNNode/Data/DestinationPhysics01"/>
      <Destination name="ASNNode/Data/DestinationControl01"/>
      <Destination name="ASNNode/Data/DestinationVideo02"/>
      <Destination name="ASNNode/Data/DestinationAudio02"/>
      <Destination name="ASNNode/Data/DestinationPhysics02"/>
      <Destination name="ASNNode/Data/DestinationControl02"/>
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceVideo"/>
		  <Source name="SyncNode/SourceAudio"/>
		  <Source name="SyncNode/SourcePhysics"/>
		  <Source name="SyncNode/SourceControl"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationVideo"/>
		  <Destination name="SyncNode/DestinationAudio"/>
		  <Destination name="SyncNode/DestinationPhysics"/>
		  <Destination name="SyncNode/DestinationControl"/>
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourceVideo"/>
		  <Source name="CodecNode/SourceAudio"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationVideo"/>
		  <Destination name="CodecNode/DestinationAudio"/>
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	  <Source name="CommNode/SourceVideo01"/>
      <Source name="CommNode/SourceAudio01"/>
      <Source name="CommNode/SourcePhysics01"/>
      <Source name="CommNode/SourceControl01"/>
	  <Source name="CommNode/SourceVideo02"/>
      <Source name="CommNode/SourceAudio02"/>
      <Source name="CommNode/SourcePhysics02"/>
      <Source name="CommNode/SourceControl02"/>
