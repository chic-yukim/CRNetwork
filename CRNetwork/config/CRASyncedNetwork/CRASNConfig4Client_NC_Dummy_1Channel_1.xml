<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="NC"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/>    <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/>    <!-- yes, no -->
    <Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourceVideo" mediatype="VideoRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceVideo" SyncNode="SyncNode/SourceVideo" CodecNode="CodecNode/SourceVideo01" CommNode="CommNode/SourceVideo"/>
    </ASNLink>
    <!--
    <ASNLink>
      <Property name ="ASNSourceAudio" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/SourceAudio" SyncNode="SyncNode/SourceAudio" CodecNode="Node" CommNode="CommNode/SourceAudio"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourcePhysics"  mediatype=""/>
      <Connection ASNNode="ASNNode/Data/SourcePhysics" SyncNode="SyncNode/SourcePhysics" CodecNode="None" CommNode="CommNode/SourcePhysics"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourceControl" mediatype=""/>
      <Connection ASNNode="ASNNode/Data/SourceControl" SyncNode="SyncNode/SourceControl" CodecNode="None" CommNode="CommNode/SourceControl"/>
    </ASNLink>    
	-->
    <ASNLink>
      <Property name="ASNDestinationVideo" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationVideo" SyncNode="SyncNode/DestinationVideo" CodecNode="CodecNode/DestinationVideo01" CommNode="CommNode/DestinationVideo"/>
    </ASNLink>
    <!--
    <ASNLink>
      <Property name="ASNDestinationAudio" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationAudio" SyncNode="SyncNode/DestinationAudio" CodecNode="None" CommNode="CommNode/DestinationAudio"/>
    </ASNLink>	
    <ASNLink>
      <Property name="ASNDestinationPhysics" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationPhysics" SyncNode="SyncNode/DestinationPhysics" CodecNode="None" CommNode="CommNode/DestinationPhysics"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationControl" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationControl" SyncNode="SyncNode/DestinationControl" CodecNode="None" CommNode="CommNode/DestinationControl"/>
    </ASNLink>    
	-->
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourceVideo"/>
      <!--
      <Source name="ASNNode/Data/SourceAudio"/>	  
      <Source name="ASNNode/Data/SourcePhysics"/>
      <Source name="ASNNode/Data/SourceControl"/>      
		-->
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationVideo"/>
      <!--
      <Destination name="ASNNode/Data/DestinationAudio"/>	  
      <Destination name="ASNNode/Data/DestinationPhysics"/>
      <Destination name="ASNNode/Data/DestinationControl"/>      
		-->
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceVideo"/>      
		  <Source name="SyncNode/SourceAudio"/>
		  <Source name="SyncNode/SourcePhysics"/>
		  <Source name="SyncNode/SourceControl"/>      
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationVideo"/>      
		  <Destination name="SyncNode/DestinationAudio"/>
		  <Destination name="SyncNode/DestinationPhysics"/>
		  <Destination name="SyncNode/DestinationControl"/>      
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourceVideo01"/>
      <!--
		  <Source name="CodecNode/SourceAudio01"/>
      -->
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationVideo01"/>
      <!--
		  <Destination name="CodecNode/DestinationAudio01"/>
      -->
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	  <Source name="CommNode/SourceVideo"/>
      <!--
      <Source name="CommNode/SourceAudio"/>	  
      <Source name="CommNode/SourcePhysics"/>
      <Source name="CommNode/SourceControl"/>      
		-->
    </SourceList>
	  <DestinationList>
		  <Destination name="CommNode/DestinationVideo" />
      <!--
		  <Destination name="CommNode/DestinationAudio" />		  
		  <Destination name="CommNode/DestinationPhysics" />
		  <Destination name="CommNode/DestinationControl" />      
		-->
	  </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig4Client_N_Dummy_1Channel_1.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig1.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfig.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
