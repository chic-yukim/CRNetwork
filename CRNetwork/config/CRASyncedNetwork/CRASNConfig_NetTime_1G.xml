<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="NS"/>     <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/>     <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/>     <!-- yes, no -->
	<Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourceControl01" mediatype="NTPMessage"/>
	  <Connection ASNNode ="ASNNode/Control/SourceControl" NetTimeNode="NetTimeNode/SourceNetTime01" CommNode="CommNode/SourceControl"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationControl01" mediatype="NTPMessage"/>
	  <Connection ASNNode ="ASNNode/Control/DestinationControl" NetTimeNode="NetTimeNode/DestinationNetTime01" CommNode="CommNode/DestinationControl"/>
    </ASNLink>   
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
		<Source name="ASNNode/Control/SourceControl"/>
    </SourceList>
    <DestinationList>
		<Destination name="ASNNode/Control/DestinationControl"/>
    </DestinationList>
  </ASyncedNetworkNodeList>
  <NetTimeNodeList>
    <SourceList>
      <Source name="NetTimeNode/SourceNetTime01"/>
    </SourceList>
    <DestinationList>
      <Destination name="NetTimeNode/DestinationNetTime01"/>
    </DestinationList>
  </NetTimeNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceControl"/>        
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationControl"/>        
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourceControl01"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationControl01"/>
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	  <Source name="CommNode/SourceControl"/>
    </SourceList>
	  <DestinationList>
		  <Destination name="CommNode/DestinationControl" />
	  </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig_NetTime_1G.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfig.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
