<?xml version="1.0" encoding="utf-8"?>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="N"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/>
    <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/>
    <!-- yes, no -->
	<Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourcePhysics01"  mediatype=""/>
      <Connection ASNNode="ASNNode/Data/SourcePhysics01" SyncNode="SyncNode/SourcePhysics" CodecNode="None" CommNode="CommNode/SourcePhysics01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationPhysics01" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationPhysics01" SyncNode="SyncNode/DestinationPhysics" CodecNode="None" CommNode="CommNode/DestinationPhysics01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourcePhysics02"  mediatype=""/>
      <Connection ASNNode="ASNNode/Data/SourcePhysics02" SyncNode="SyncNode/SourcePhysics" CodecNode="None" CommNode="CommNode/SourcePhysics02"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationPhysics02" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationPhysics02" SyncNode="SyncNode/DestinationPhysics" CodecNode="None" CommNode="CommNode/DestinationPhysics02"/>
    </ASNLink>
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourcePhysics01"/>
      <Source name="ASNNode/Data/SourcePhysics02"/>
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationPhysics01"/>
      <Destination name="ASNNode/Data/DestinationPhysics02"/>
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceVideo"/>
		  <Source name="SyncNode/SourceAudio"/>
		  <Source name="SyncNode/SourcePhysics"/>
		  <Source name="SyncNode/SourceControl"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationVideo"/>
		  <Destination name="SyncNode/DestinationAudio"/>
		  <Destination name="SyncNode/DestinationPhysics"/>
		  <Destination name="SyncNode/DestinationControl"/>
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourceVideo"/>
		  <Source name="CodecNode/SourceAudio"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationVideo"/>
		  <Destination name="CodecNode/DestinationAudio"/>
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
      <Source name="CommNode/SourcePhysics01"/>
      <Source name="CommNode/SourcePhysics02"/>
    </SourceList>
	  <DestinationList>
      	<Destination name="CommNode/DestinationPhysics01" />
      	<Destination name="CommNode/DestinationPhysics02" />
	  </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig4Server_3ch.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfig.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
