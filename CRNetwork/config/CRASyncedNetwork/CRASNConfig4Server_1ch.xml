<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="N"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/>
    <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/>
    <!-- yes, no -->
	<Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourceVideo01" mediatype="VideoRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceVideo01" SyncNode="SyncNode/SourceVideo" CodecNode="CodecNode/SourceVideo" CommNode="CommNode/SourceVideo01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationVideo01" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationVideo01" SyncNode="SyncNode/DestinationVideo" CodecNode="CodecNode/DestinationVideo" CommNode="CommNode/DestinationVideo01"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNSourceVideo02" mediatype="VideoRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceVideo02" SyncNode="SyncNode/SourceVideo" CodecNode="CodecNode/SourceVideo" CommNode="CommNode/SourceVideo02"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationVideo02" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationVideo02" SyncNode="SyncNode/DestinationVideo" CodecNode="CodecNode/DestinationVideo" CommNode="CommNode/DestinationVideo02"/>
    </ASNLink>
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourceVideo01"/>
      <Source name="ASNNode/Data/SourceVideo02"/>
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationVideo01"/>
      <Destination name="ASNNode/Data/DestinationVideo02"/>
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceVideo"/>
		  <Source name="SyncNode/SourceAudio"/>
		  <Source name="SyncNode/SourcePhysics"/>
		  <Source name="SyncNode/SourceControl"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationVideo"/>
		  <Destination name="SyncNode/DestinationAudio"/>
		  <Destination name="SyncNode/DestinationPhysics"/>
		  <Destination name="SyncNode/DestinationControl"/>
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourceVideo"/>
		  <Source name="CodecNode/SourceAudio"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationVideo"/>
		  <Destination name="CodecNode/DestinationAudio"/>
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	  <Source name="CommNode/SourceVideo01"/>
	  <Source name="CommNode/SourceVideo02"/>
    </SourceList>
	  <DestinationList>
		<Destination name="CommNode/DestinationVideo01" />
		<Destination name="CommNode/DestinationVideo02" />
	  </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig4Server_1ch.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfig.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
