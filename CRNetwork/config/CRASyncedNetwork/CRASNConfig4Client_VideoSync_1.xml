<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="NCS"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/>
    <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/>
    <!-- yes, no -->
	<Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourceVideo" mediatype="RawVideoRGB"/>
      <Connection ASNNode ="ASNNode/Data/SourceVideo" SyncNode="SyncNode/SourceVideo01" CodecNode="CodecNode/SourceVideo01" CommNode="CommNode/SourceVideo"/>
    </ASNLink>    
    <ASNLink>
      <Property name="ASNDestinationVideo" mediatype="RawVideoRGB"/>
      <Connection ASNNode ="ASNNode/Data/DestinationVideo" SyncNode="SyncNode/DestinationVideo01" CodecNode="CodecNode/DestinationVideo01" CommNode="CommNode/DestinationVideo"/>
    </ASNLink>    
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourceVideo"/>      
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationVideo"/>      
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceVideo01"/>      
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationVideo01"/>      
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourceVideo01"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationVideo01"/>
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	    <Source name="CommNode/SourceVideo"/>          
     </SourceList>
	 <DestinationList>
	   <Destination name="CommNode/DestinationVideo" />    
	 </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig4Client_VideoSync_1.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig1.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfig1.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
