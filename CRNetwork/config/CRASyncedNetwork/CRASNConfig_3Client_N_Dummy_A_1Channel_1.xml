<?xml version="1.0" encoding="utf-8"?>
<ASNConfig>
  <InfoList>
    <Info name ="ASNVersion" value="1.0"/>
    <Info name ="Configuration" value="CRASN_Example"/>
    <Info name ="ASNType" value="N"/>
    <!-- NCS=N+C+S, NC=N+C, NS=N+S, N, NCS = (N)etwork + (C)odec + (S)ynchronization -->
    <Info name ="CRSFHeader" value="Detach"/> <!--Attach, Detach-->
    <Info name ="PrintError" value="yes"/> <!-- yes, no -->
	<Info name ="ASNStateManagerFileName" value="config@CRASyncedNetwork@State@ASNStateManager.xml"/>
  </InfoList>
  <ASNLinkList>
    <ASNLink>
      <Property name="ASNSourceAudio" mediatype="AudioRawDepth"/>
      <Connection ASNNode ="ASNNode/Data/SourceAudio" SyncNode="SyncNode/SourceAudio" CodecNode="CodecNode/SourceAudio01" CommNode="CommNode/SourceAudio"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationAudio21" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationAudio21" SyncNode="SyncNode/DestinationAudio1" CodecNode="CodecNode/DestinationAudio01" CommNode="CommNode/DestinationAudio21"/>
    </ASNLink>
    <ASNLink>
      <Property name="ASNDestinationAudio31" mediatype=""/>
      <Connection ASNNode ="ASNNode/Data/DestinationAudio31" SyncNode="SyncNode/DestinationAudio2" CodecNode="CodecNode/DestinationAudio02" CommNode="CommNode/DestinationAudio31"/>
    </ASNLink>
  </ASNLinkList>
  <ASyncedNetworkNodeList>
    <SourceList>
      <Source name="ASNNode/Data/SourceAudio"/>
    </SourceList>
    <DestinationList>
      <Destination name="ASNNode/Data/DestinationAudio21"/>
      <Destination name="ASNNode/Data/DestinationAudio31"/>
    </DestinationList>
  </ASyncedNetworkNodeList>
  <SynchronizationNodeList>
	  <SourceList>
		  <Source name="SyncNode/SourceAudio"/>        
	  </SourceList>
	  <DestinationList>
		  <Destination name="SyncNode/DestinationAudio"/>        
	  </DestinationList>
  </SynchronizationNodeList>
  <CodecNodeList>
	  <SourceList>
		  <Source name="CodecNode/SourceAudio01"/>
	  </SourceList>
	  <DestinationList>
		  <Destination name="CodecNode/DestinationAudio01"/>
	  </DestinationList>
  </CodecNodeList>
  <CommunicationNodeList>
	  <SourceList>
	  <Source name="CommNode/SourceAudio"/>
    </SourceList>
	  <DestinationList>
		  <Destination name="CommNode/DestinationAudio21" />
		  <Destination name="CommNode/DestinationAudio31" />
	  </DestinationList>
  </CommunicationNodeList>
  <ConfigurationList>
    <Configuration name="Network" value="config@CRNetwork@CRNetConfig_3Client_N_Dummy_A_1Channel_1.xml"/>
    <Configuration name="Codec" value="config@CRCodec@CRCodecConfig.xml"/>
    <Configuration name="Synchronization" value="config@CRSynchronization@CRSyncConfig.xml"/>
    <Configuration name="MediaType" value="config@CRSynchronization@Syncer@CRSyncMediaType.xml"/>
  </ConfigurationList>
</ASNConfig>
