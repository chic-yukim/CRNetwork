<StateMachine>
	<Configuration>
 		<Property startState="State:Network:Hello" bufferSize="40" />
	</Configuration>
<TransitionRuleList>
		<TransitionRule name="Trans:1" fromState="State:Network:Hello" event="Event:Network:Prepare" toState="State:Network:Preparing" type="Determined" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:2" fromState="State:Network:Preparing" event="Event:Network:Prepare" toState="State:Network:Preparing" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:3" fromState="State:Network:Preparing" event="Event:Network:PreparedAll" toState="State:Network:Prepared" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:3-1" fromState="State:Network:Preparing" event="Event:Network:Error" toState="State:Network:Error" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:4" fromState="State:Network:Prepared" event="Event:Network:Start" toState="State:Network:Starting" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:4-1" fromState="State:Network:Prepared" event="Event:Network:Error" toState="State:Network:Error" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:4-2" fromState="State:Network:Prepared" event="Event:Network:Finalize" toState="State:Network:Finalizing" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:5" fromState="State:Network:Starting" event="Event:Network:Start" toState="State:Network:Starting" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:6" fromState="State:Network:Starting" event="Event:Network:StartedAll" toState="State:Network:Started" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:6-1" fromState="State:Network:Starting" event="Event:Network:Error" toState="State:Network:Error" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:7" fromState="State:Network:Started" event="Event:Network:Stop" toState="State:Network:Stopping" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:7-1" fromState="State:Network:Started" event="Event:Network:Error" toState="State:Network:Error" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:8" fromState="State:Network:Stopping" event="Event:Network:Stop" toState="State:Network:Stopping" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:9" fromState="State:Network:Stopping" event="Event:Network:StoppedAll" toState="State:Network:Stopped" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:9-1" fromState="State:Network:Stopping" event="Event:Network:Error" toState="State:Network:Error" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:10" fromState="State:Network:Stopped" event="Event:Network:Start" toState="State:Network:Starting" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:10-1" fromState="State:Network:Stopped" event="Event:Network:Error" toState="State:Network:Error" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:11" fromState="State:Network:Starting" event="Event:Network:Stop" toState="State:Network:Stopping" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:12" fromState="State:Network:Stopped" event="Event:Network:Finalize" toState="State:Network:Finalizing" type="Conditional" err="Error:NetworkStateError"/>
    <TransitionRule name="Trans:13" fromState="State:Network:Finalizing" event="Event:Network:Finalize" toState="State:Network:Finalizing" type="Conditional" err="Error:NetworkStateError"/>
    <TransitionRule name="Trans:14" fromState="State:Network:Finalizing" event="Event:Network:FinalizedAll" toState="State:Network:Finalized" type="Conditional" err="Error:NetworkStateError"/>
	<!--
		<TransitionRule name="Trans:15" fromState="State:Network:Finalized" event="Event:Network:Exit" toState="State:Network:Bye" type="Determined" err="Error:NetworkStateError"/>
	-->
		<TransitionRule name="Trans:16" fromState="State:Network:Finalized" event="Event:Network:Error" toState="State:Network:Error" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:17" fromState="State:Network:Error" event="Event:Network:Finalize" toState="State:Network:Finalizing" type="Conditional" err="Error:NetworkStateError"/>
		<TransitionRule name="Trans:18" fromState="State:Network:Error" event="Event:Network:Stop" toState="State:Network:Stopping" type="Conditional" err="Error:NetworkStateError"/>
	</TransitionRuleList>
	<EventList>
		<Event name="Event:Network:Prepare" location="Outer" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:PreparedAll" location="Inner" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:Start" location="Outer" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:StartedAll" location="Inner" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:Stop" location="Outer" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:StoppedAll" location="Inner" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:Finalize" location="Outer" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:FinalizedAll" location="Inner" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:Exit" location="Outer" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
		<Event name="Event:Network:Error" location="Inner" process="Sync"/>  <!-- location=Inner/Outer, process=Sync/ASync -->
	</EventList>
	<StateList>
		<State name="State:Network:Hello"/>
		<State name="State:Network:Preparing"/>
		<State name="State:Network:Prepared"/>
		<State name="State:Network:Starting"/>
		<State name="State:Network:Started"/>
		<State name="State:Network:Stopping"/>
		<State name="State:Network:Stopped"/>
		<State name="State:Network:Finalizing"/>
		<State name="State:Network:Finalized"/>
		<State name="State:Network:Bye"/>
		<State name="State:Network:Error"/>
	</StateList>
	<ErrorList>
		<Error name="Error:TBD"/>
		<Error name="Error:NetworkStateError"/>
	</ErrorList>
</StateMachine>
